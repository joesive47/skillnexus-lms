{
  "name": "uppowerskill-lms",
  "version": "1.0.0",
  "description": "upPowerSkill - Professional Learning Management System",
  "homepage": "https://upPowerSkill.com",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "vercel-build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate deploy",
    "db:seed": "tsx prisma/seed.ts",
    "db:setup-postgresql": "node scripts/setup-postgresql.js",
    "db:reset": "prisma migrate reset --force",
    "migrate:production": "node scripts/migrate-production.js",
    "db:connect-production": "node scripts/connect-production-db.js",
    "build:clean": "rm -rf .next && npm run build",
    "import:knowledge": "tsx scripts/import-knowledge.ts",
    "load-test": "node scripts/load-test.js",
    "stress-test": "node scripts/stress-test.js",
    "performance:check": "node -e \"fetch('http://localhost:3000/api/metrics').then(r=>r.json()).then(console.log)\"",
    "docker:performance": "docker-compose -f docker-compose.performance.yml up -d",
    "k8s:deploy": "kubectl apply -f k8s/deployment.yml",
    "gcloud:deploy": "gcloud app deploy",
    "gcloud:logs": "gcloud app logs tail"
  },
