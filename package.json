{
  "name": "uppowerskill-lms",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run build:check && next build",
    "build:local": "npm run build:check && next build && node scripts/post-build.js",
    "build:check": "node scripts/essential/check-prisma.js || prisma generate",
    "build:fast": "cross-env SKIP_TYPE_CHECK=true SKIP_LINT=true next build",
    "build:ultra": "npm run clean:cache && npm run build:fast",
    "build:safe": "node fix-build.js && npm run db:generate && next build",
    "build:production": "cross-env NODE_ENV=production npm run build",
    "start": "next start",
    "lint": "next lint",
    "clean:all": "node cleanup-project.js",
    "clean:cache": "rimraf .next node_modules/.cache .turbo .swc",
    "clean:logs": "rimraf *.log debug.log error.log",
    "clean:build": "rimraf .next dist build *.tsbuildinfo",
    "optimize": "node build-optimizer.js",
    "analyze": "cross-env ANALYZE=true npm run build",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:seed": "node -r esbuild-register prisma/seed.ts || tsx prisma/seed.ts || node prisma/seed.js",
    "db:seed:analytics": "tsx scripts/seed-analytics.ts",
    "db:studio": "prisma studio",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "load-test": "tsx scripts/essential/load-test.ts",
    "security:scan": "tsx scripts/essential/security-scan.ts",
    "test:import": "node test-import-system.js",
    "setup": "npm run db:generate && npm run db:push && npm run db:seed",
    "setup:postgresql": "node scripts/essential/setup-postgresql.js",
    "build:clean": "rimraf .next && npm run db:generate && next build",
    "build:force": "rimraf .next node_modules/.cache && npm install && npm run db:generate && next build",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:restart": "docker-compose restart",
    "docker:clean": "docker-compose down -v && docker system prune -f",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:setup": "npm run docker:build && npm run docker:up",
    "docker:init": "node scripts/docker-setup.bat",
    "docker:env": "copy .env.docker .env",
    "docker:db": "npm run db:generate && npm run db:push && npm run db:seed",
    "docker:full": "npm run docker:env && npm run docker:up && timeout /t 10 && npm run docker:db",
    "deploy:all": "npx prisma generate && npx prisma db push && npx tsx prisma/deploy-all.ts",
    "deploy:verify": "npx tsx prisma/verify-courses.ts",
    "deploy:quick": "npx tsx prisma/deploy-all.ts"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.7.4",
